<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:custom="http://metro.mahapps.com/winfx/xaml/shared"
                    xmlns:editorControl="clr-namespace:miRobotEditor.EditorControl;assembly=miRobotEditor.EditorControl"
                    xmlns:views="clr-namespace:miRobotEditor.UI.Views;assembly=miRobotEditor.UI">
    <custom:ThicknessToDoubleConverter x:Key="ThicknessToDoubleConverter" />
    <AlternationConverter x:Key="AlternationConverter" />



    <DataTemplate DataType="{x:Type editorControl:EditorOptions}">
        <views:EditorOptionsView/>
    </DataTemplate>
</ResourceDictionary>